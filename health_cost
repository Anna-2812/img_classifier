        "# Import data\n",
        "!wget https://cdn.freecodecamp.org/project-data/health-costs/insurance.csv\n",
        "dataset = pd.read_csv('insurance.csv')\n",
        "dataset.tail()"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "LcopvQh3X-kX"
      },
      "outputs": [],
      "source": [
        "from keras.utils import to_categorical\n",
        "\n",
        "dataset.dropna()\n",
        "dataset['region'] = pd.factorize(dataset['region'])[0]\n",
        "dataset['sex'] = pd.factorize(dataset['sex'])[0]\n",
        "dataset['smoker'] = pd.factorize(dataset['smoker'])[0]\n",
        "\n",
        "train_features = dataset.sample(frac=0.8, random_state=0)\n",
        "\n",
        "test_features = dataset.drop(train_features.index)\n",
        "\n",
        "train_labels =train_features['expenses']\n",
        "\n",
        "test_labels = test_features['expenses']\n",
        "\n",
        "train_features = train_features.drop('expenses', axis = 1)\n",
        "\n",
        "test_features = test_features.drop('expenses',axis = 1)\n",
        "\n",
        "model = tf.keras.models.Sequential([\n",
        "    tf.keras.layers.Input(shape=(len(train_features.keys()),)),\n",
        "    tf.keras.layers.BatchNormalization(),\n",
        "    tf.keras.layers.Dense(64, activation='relu'),\n",
        "    tf.keras.layers.Dense(64, activation='relu'),\n",
        "    tf.keras.layers.Dense(1)\n",
        "])\n",
        "\n",
        "model.compile(\n",
        "    optimizer= tf.keras.optimizers.RMSprop(0.05),\n",
        "    loss='mse',\n",
        "    metrics=['mae', 'mse']\n",
        ")\n",
        "\n",
        "model.summary()\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "model.fit(train_features,train_labels,epochs = 100)"
      ],
      "metadata": {
        "id": "pxFuhgDv1AKM"
      },
      "execution_count": null,
      "outputs": []
    },
